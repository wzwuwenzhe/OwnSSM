    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deady.dao.ClientDAO">
    <insert id="insertClient">
        INSERT  INTO client (id,store_id, name,phone)
        VALUES (#{id},#{storeId}, #{cusName},#{cusPhone})
    </insert>
    

</mapper>